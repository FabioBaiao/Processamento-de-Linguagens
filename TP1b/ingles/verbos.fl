%option noyywrap
%{
	#include <glib.h>
	#include <string.h>
	void imprimeVerbos();
	GPtrArray *verbos;
	int n = 0;
%}
ALPHA	[a-zA-Z]+
%%
to\ {ALPHA} 	{char *v = strdup(yytext+3); g_ptr_array_add(verbos, v);n++;}
%%
int main(){
	verbos = g_ptr_array_new();
	yylex();
	g_ptr_array_sort(verbos, (GCompareFunc)strcmp);
	imprimeVerbos();
}

void imprimeVerbos(){
	int i;
	for (i = 0; i < n; i++){
		char *v = g_ptr_array_index(verbos, i);
		printf("%s\n", v);
	}
}